{
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*"],
      "@app": ["./src/app"],
      "@app/*": ["./src/app/*"],
      "@apis": ["./src/apis"],
      "@apis/*": ["./src/apis/*"],
      "@assets": ["./src/assets"],
      "@assets/*": ["./src/assets/*"],
      "@constants": ["./src/constants"],
      "@constants/*": ["./src/constants/*"],
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@themes": ["./src/themes"],
      "@themes/*": ["./src/themes/*"],
      "@stores": ["./src/stores"],
      "@stores/*": ["./src/stores/*"],
      "@providers": ["./src/providers"],
      "@providers/*": ["./src/providers/*"],
      "@utilities": ["./src/utilities"],
      "@utilities/*": ["./src/utilities/*"],
      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"]
    }
  },
  "typeAcquisition": {
    "include": ["jest"]
  }
}
